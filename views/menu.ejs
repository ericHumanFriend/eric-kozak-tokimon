<!DOCTYPE html>
<html>

<div id=navigate>
    <a href="/">Menu</a>
    <a href="/add">Add</a>
</div>

<head>
    <title> Menu </title>
    <link rel="stylesheet" href="css/tokimon.css">
</head>

<body>

<table>
    <% tokimon.forEach((item, index) => { %>
        <tr>
            <td> <a href="Tokimon/<%= item.id %>"> <%= item.name %> </a> <td>
            <td>
                <form action="/edit/<%= item.id %>" method="post">
                    <input type="submit" value="Edit">
                </form>
            </td>
            <td>
                <form action="/delete/<%= item.id %>" method="post">
                    <input type="submit" value="Delete">
                </form>
            </td>

        </tr>
    <% }) %>
</table>

</body>

</html>